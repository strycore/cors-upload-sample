{
  "ecmaFeatures": {
    "modules": true,
    "classes": true
  },
  "rules": {
    "quotes": "simple",
    "indent": [2, 2],
    "eol-last": false,
    "no-console": false,
    "new-cap": false,
    "spaced-line-comment": ["always"],
    "valid-jsdoc": [2, {
      "requireReturn": false
    }],
    "dot-notation": false
  },
  "env": {
    "browser": true,
    "es6": true
  }
}